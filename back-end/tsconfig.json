{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
      "lib": ["es2021"],
      "target": "es2021",
      "rootDir": "./",
      "outDir": "./dist",

      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "preserveConstEnums": true,
    }
}